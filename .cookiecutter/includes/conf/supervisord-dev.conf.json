{
  "programs": {
    "make_db": {
      "command": "bin/make_db",
      "startsecs": "0"
    },
    "web": {
      "command": "gunicorn -c conf/gunicorn/dev.conf.py --paste conf/development.ini"
    },
    "nginx": {
      "command": "docker compose run --rm --service-ports nginx-proxy",
      "stopsignal": "TERM"
    },
    "assets": {
      "command": "node_modules/.bin/gulp watch"
    }
  }
}
